{
  "manifestVersion": 1,
  "executable": "plugin.py",
  "protocol_version": "2.0",
  "persistent": true,
  "passthrough": true,
  "name": "ifttt",
  "description": "Trigger IFTTT events.",
  "functions": [
    {
      "name": "trigger_gaming_setup",
      "description": "Triggers your custom IFTTT gaming setup \u00e2\u20ac\u201d ideal for commands like 'It's gametime', 'It's game time', 'start my gaming scene', or 'activate gamer mode'.",
      "tags": [
        "gaming",
        "gametime",
        "game time",
        "setup",
        "lights",
        "gamer",
        "scene",
        "rgb",
        "start",
        "activate"
      ],
      "properties": {
        "event_name": {
          "type": "string",
          "description": "The IFTTT event name to trigger"
        }
      }
    }
  ],
  "tethered": true,
  "tether_config": {
    "heartbeat_interval": 5,
    "heartbeat_timeout": 20,
    "onboarding_timeout": 60,
    "allow_passthrough": true
  }
}